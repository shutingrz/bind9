$TTL 300	; 5 minutes
@			IN SOA	mname1. . (
				2000042407 ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
			NS	ns
			NSEC3PARAM 1 0 1 D399EAAB
ns			A	10.53.0.3

a			A	10.0.0.1
b			A	10.0.0.2
d			A	10.0.0.4
z			A	10.0.0.26
a.a.a.a			A	10.0.0.3
child			NS	ns2.example.
insecure		NS	ns.insecure
ns.insecure 		A	10.53.0.3
secure			NS	ns.secure
ns.secure 		A	10.53.0.3
nsec3			NS	ns.nsec3
ns.nsec3 		A	10.53.0.3
optout			NS	ns.optout
ns.optout 		A	10.53.0.3
